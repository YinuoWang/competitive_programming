#include <vector>

using namespace std;

class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        int vecSize = nums.size();
        for (int i=0; i<vecSize; ++i){
            for (int k = i + 1; k<vecSize; ++k){
                if (nums[i] + nums[k] == target){
                    vector<int>returnArray = {i,k};
                    return returnArray;
                }
            }
        }
    }
};